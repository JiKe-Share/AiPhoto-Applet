@font-face {font-family: "iconfont";
  src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2')
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-close:before {
  content: "\e622";
}
.wyb-popup-box{
	border-radius: 20rpx;
	position: fixed;
	transition-timing-function: ease-out;
	transition-property: opacity, -webkit-transform;
	transition-property: opacity, transform;
	transition-property: opacity, transform, -webkit-transform;
	/* overflow: hidden; */
}
.wyb-popup-container {
	position: relative;
	box-sizing: border-box;
	border-radius: 20rpx;
}
.wyb-popup-slot {
	border-radius: 20rpx;
	text-align: center;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	height: 100%;
	background-color: rgba(0,32,57,.9);
}
.wyb-popup-mask {
	position: fixed;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	transition-timing-function: ease;
	transition-property: opacity, -webkit-transform;
	transition-property: opacity, transform;
	transition-property: opacity, transform, -webkit-transform;
	-webkit-backdrop-filter: blur(10px) brightness(110%);
	        backdrop-filter: blur(10px) brightness(110%);
}
.wyb-popup-close {
	text-align: center;
		position: absolute;
	    width: 70rpx;
	    height: 70rpx;
	    border: 2rpx solid #00ffe1;
		left: 50%;
		bottom:-100%;
	    margin-left: -35rpx;
	    border-radius: 100%;
		color: #00ffe1;
		/* background-color: #00FFE1; */
		z-index: 99999;
		padding-top: 5rpx;
}
.ad{
	position: absolute;
	z-index: 99999;
}
.icon-close{
	font-size: 50rpx;
}
.wyb-popup-custom-close {
	left: 0;
	top: 0;
	position: absolute;
}

